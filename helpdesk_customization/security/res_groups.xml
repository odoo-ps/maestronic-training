<?xoml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- New group: internal user++ getting extra privileges-->
    <record id="group_repair_user" model="res.groups">
        <field name="name">Repair Basic User</field>
    </record>
    <record id="group_customer" model="res.groups">
        <field name="name">Customer</field>
        <field name="implied_ids" eval="[(4, ref('group_repair_user'))]"/>
    </record>
    <record id="group_user_plus" model="res.groups">
        <field name="name">Internal User+</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
